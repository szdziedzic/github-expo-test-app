{
  "cli": {
    "version": ">= 3.9.1"
  },
  "build": {
    "base": {
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal"
    },
    "development-simulator": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal"
    },
    "preview-simulator": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base"
    },
    "custom-preview": {
      "distribution": "internal",
      "android": {
        "config": "android-internal.yml"
      },
      "ios": {
        "simulator": true,
        "config": "ios-simulator.yml"
      }
    },
    "custom-production": {
      "android": {
        "config": "android-production.yml"
      },
      "ios": {
        "config": "ios-production.yml"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
